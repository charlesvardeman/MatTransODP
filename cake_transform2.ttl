# filename: transform.ttl

@prefix batterTransformation:<http://CakeExample.com/batterTransformation#> .
@prefix BakeACake: <http://CakeExample.com/BakeACake#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:batterTransformation 	a							:MaterialTransformation ;
						:hasInput					:Resource1, :Resource2, :Resource3, :Resource4, :Resource5, :Resource6, Resource7 ;
						:hasInput					:Catalyst1, :Catalyst2, :Catalyst3, :Catalyst4 ;
						:hasDuration				:OWL:time ;
						:hasOutput					:Artifact1, :ByProduct1 ;

						
						
:Resource1				:a							:Resource ;
						:isA	 					:Material1 ;
:Material1				a							:Material ;
						a							:sugar:"1 cup sugar" ;	

:Resource2				:a							:Resource ;
						:isA	 					:Material2 ;
:Material2				a							:Material ;
						a							:butter:"1/2 cup butter" ;
						
:Resource3				:a							:Resource ;
						:isA	 					:Material3 ;
:Material3				a							:Material ;
						a							:egg:"2 eggs" ;
							
:Resource4				:a							:Resource ;
						:isA	 					:Material4 ;
:Material4				a							:Material ;
						a							:vanilla:"2 teaspoons vanilla extract" ;

:Resource5				:a							:Resource ;
						:isA	 					:Material5 ;
:Material5				a							:Material ;
						a							:flour:"1-1/2 cups flour" ;

:Resource6				:a							:Resource ;
						:isA	 					:Material6 ;
:Material6				a							:Material ;
						a							:baking_power:"1-3/4 teaspoons baking powder" ;

:Resource7				:a							:Resource ;
						:isA	 					:Material7 ;
:Material7				a							:Material ;
						a							:milk:"1/2 cup milk" ;						
						
						

:Catalyst1				:consumes				    :Work/Energy
						
:Catalyst2				:a							:Catalyst ;
						:isA	 					:MaterialA ;
:MaterialA				a							:electricity:"wall outlet" ;	

:Catalyst3				:a							:Catalyst ;
						:isA	 					:MaterialB ;
:MaterialB				a							:measuring_tools:"measuring cups" ;	

:Catalyst3				:a							:Catalyst ;
						:isA	 					:MaterialC ;
:MaterialC				a							:measuring_tools:"measuring spoons" ;	

:Catalyst4				:a							:Catalyst ;
						:isA	 					:MaterialD ;
:MaterialD				a							:mixer:"electric mixer" ;					
						
:Artifact1				a							:Artifact ;
						:hasOutput					:batter ;
						
:ByProduct1				a							:ByProduct ;
						:hasOutput					:trash ;		
						
:OWL:time				a							:hour ;
						
						
						
						
						
